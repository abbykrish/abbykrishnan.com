{
  "name": "abbykrishnan-com",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts --serve\" \"esbuild src/ts/main.ts --bundle --outfile=_site/js/main.js --watch\"",
    "build": "tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts && esbuild src/ts/main.ts --bundle --outfile=_site/js/main.js --minify",
    "build:fresh": "rm -rf .cache && npm run build"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@types/node": "^22.19.7",
    "concurrently": "^9.0.0",
    "esbuild": "^0.24.0",
    "tsx": "^4.21.0",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "@11ty/eleventy-fetch": "^5.1.1"
  }
}
